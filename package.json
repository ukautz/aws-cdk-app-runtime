{
  "name": "@ukautz/aws-cdk-app-runtime",
  "version": "2.0.0-alpha1",
  "homepage": "https://github.com/ukautz/aws-cdk-app-runtime",
  "bugs": {
    "url": "https://github.com/ukautz/aws-cdk-app-runtime/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "build": "ttsc",
    "watch": "ttsc -w",
    "test": "jest",
    "test-update": "jest -u",
    "clean": "find test/ \\( -name \"*.js\" -or -name \"*.d.ts\" \\) -delete && find lib/ \\( -name \"*.js\" -or -name \"*.d.ts\" \\) -delete",
    "create-schema": "npx typescript-json-schema \"lib/specs.ts\" ClusterSpecs",
    "package": "tar zcvf app-runtime-library.$(cat package.json | jq .version -r).tgz --transform 's+^+package/+' package.json dist/"
  },
  "devDependencies": {
    
    "@aws-cdk/cloud-assembly-schema": "2.74.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "aws-cdk-lib": "2.74.0",
    "constructs": "^10.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-transformer-keys": "^0.4.4",
    "typescript": "~4.9.5",
    "ttypescript": "~1.5.15"
  },
  "peerDependencies": {
    "@aws-cdk/cloud-assembly-schema": "^2.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}